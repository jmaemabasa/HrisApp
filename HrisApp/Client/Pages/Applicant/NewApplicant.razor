                            <MudText Typo="Typo.body2">Have you ever been terminated or dismissed by any of your previous employer/s?</MudText>
                        </div>
                        <div class="col-sm col-md-4 d-flex justify-content-center border-bottom">
                            <MudCheckBox Class="@clsQ3" @bind-Checked="@YesCB3">Yes</MudCheckBox>
                            <MudCheckBox Class="@clsQ3" @bind-Checked="@NoCB3">No</MudCheckBox>
                        </div>
                        <div class="col-sm col-md-4 border-bottom ">
                            <MudTextField @bind-Value="selfdec.Q3Dtls" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                        </div>
                    </div>
                    <div class="row mt-3 mb-2 ">
                        <div class="col-sm col-md-4 border-bottom ">
                            <MudText Typo="Typo.body2">Do you have pending criminal, civil, labor or administrative case filed against you/ and/ or you filed against other parties?</MudText>
                        </div>
                        <div class="col-sm col-md-4 d-flex justify-content-center border-bottom">
                            <MudCheckBox Class="@clsQ4" @bind-Checked="@YesCB4">Yes</MudCheckBox>
                            <MudCheckBox Class="@clsQ4" @bind-Checked="@NoCB4">No</MudCheckBox>
                        </div>
                        <div class="col-sm col-md-4  border-bottom">
                            <MudTextField @bind-Value="selfdec.Q4Dtls" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                        </div>
                    </div>
                    <div class="row mt-3 mb-2 ">
                        <div class="col-sm col-md-4 border-bottom ">
                            <MudText Typo="Typo.body2">Are you willing to work on shifting, overtime and holidays?</MudText>
                        </div>
                        <div class="col-sm col-md-4 d-flex justify-content-center border-bottom">
                            <MudCheckBox Class="@clsQ5" @bind-Checked="@YesCB5">Yes</MudCheckBox>
                            <MudCheckBox Class="@clsQ5" @bind-Checked="@NoCB5">No</MudCheckBox>
                        </div>
                        <div class="col-sm col-md-4 border-bottom ">
                            <MudTextField @bind-Value="selfdec.Q5Dtls" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                        </div>
                    </div>
                    <div class="row  mt-3 mb-2 ">
                        <div class="col-sm col-md-4  border-bottom">
                            <MudText Typo="Typo.body2">Are you willing to be assigned in any of the Company's Satellite Offices?</MudText>
                        </div>
                        <div class="col-sm col-md-4 d-flex justify-content-center border-bottom">
                            <MudCheckBox Class="@clsQ6" @bind-Checked="@YesCB6">Yes</MudCheckBox>
                            <MudCheckBox Class="@clsQ6" @bind-Checked="@NoCB6">No</MudCheckBox>
                        </div>
                        <div class="col-sm col-md-4 border-bottom ">
                            <MudTextField @bind-Value="selfdec.Q6Dtls" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                        </div>
                    </div>
                    <div class="row mt-3 mb-2">
                        <div class="col-sm col-md-4 border-bottom ">
                            <MudText Typo="Typo.body2">Do you have tattoo in any part of your body?</MudText>
                        </div>
                        <div class="col-sm col-md-4 d-flex justify-content-center border-bottom">
                            <MudCheckBox Class="@clsQ7" @bind-Checked="@YesCB7">Yes</MudCheckBox>
                            <MudCheckBox Class="@clsQ7" @bind-Checked="@NoCB7">No</MudCheckBox>
                        </div>
                        <div class="col-sm col-md-4  border-bottom">
                            <MudTextField @bind-Value="selfdec.Q7Dtls" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                        </div>
                    </div>
                </MudCardContent>
            </MudCard>

            <MudDivider DividerType="DividerType.Middle" Class="my-4" />

            <MudCard Class="py-3 px-3 pb-2" Elevation="0">
                <MudCardContent>
                    <span class="spantitle">Person to Notify In Case of Emergency</span>
                    <div class="row mb-2">
                        <div class="col-md-3">
                            <MudText Typo="Typo.body2">Name</MudText>
                            <MudTextField @bind-Value="@applicant.App_EmerName" Class="@clsEmerName" Variant="Variant.Outlined" Margin="Margin.Dense" />
                            <span class="mud-input-helper-text mud-input-error">@mesEmerName</span>
                        </div>
                        <div class="col-md-3">
                            <MudText Typo="Typo.body2">Relationship</MudText>
                            <MudSelect @bind-Value="applicant.App_EmerRelationshipId" For=@(() => applicant.App_EmerRelationshipId) Immediate=true Class="@clsEmerRel" Variant="Variant.Outlined" Margin="Margin.Dense">
                                     <MudSelectItem Value=0>---Select---</MudSelectItem>
                                     @foreach (var val in EmerRelationshipsL)
                                    {
                                        <MudSelectItem Value="@val.Id">@val.Name</MudSelectItem>
                                    }
                                </MudSelect>
                            <span class="mud-input-helper-text mud-input-error">@mesEmerRel</span>
                         </div>
                         <div class="col-md-3">
                            <MudText Typo="Typo.body2">Address</MudText>
                            <MudTextField @bind-Value="@applicant.App_EmerAddress" Class="@clsEmerAdd" Variant="Variant.Outlined" Margin="Margin.Dense" />
                            <span class="mud-input-helper-text mud-input-error">@mesEmerAdd</span>
                        </div>
                        <div class="col-md-3">
                            <MudText Typo="Typo.body2">Contact No.</MudText>
                            <MudTextField @bind-Value="@applicant.App_EmerMobNum" Class="@clsEmerNo" Variant="Variant.Outlined" Margin="Margin.Dense" />
                            <span class="mud-input-helper-text mud-input-error">@mesEmerNo</span>
                        </div>
                     </div>
                </MudCardContent>
            </MudCard>
            <MudItem xs="12" Class="d-flex justify-content-between mt-5">
                <MudButton StartIcon="@Icons.Material.Rounded.NavigateBefore" Class="btnnext" OnClick="@(()=> Activate(3))" style="background-color: #C5CFD2;">BACK</MudButton>
                <MudButton EndIcon="@Icons.Material.Rounded.Save" Class="btnnext" OnClick="CreateApplicant">Submit</MudButton>
            </MudItem>
        </MudTabPanel>
    </MudTabs>
        

</div>


