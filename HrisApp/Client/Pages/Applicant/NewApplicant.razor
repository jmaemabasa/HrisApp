@layout NewApplicantLayout
@page "/application"


@inject IApplicantService ApplicantService
@inject IAppAddressService AppAddressService
@inject IAppEducService AppEducService
@inject IAppLicenseTrainingService AppLicenseTrainingService
@inject IStaticService StaticService
@inject IAppSibChildService AppSibChildService
@inject IJSRuntime JSRuntime
@inject SweetAlertService Swal
@inject GlobalConfigService GlobalConfigService
@inject NavigationManager _navigationManager

<PageTitle>New Applicant</PageTitle>

<div class="tabdiv">
    <div class="d-flex justify-content-between align-items-center mb-2">
        <span class="spantitle" style="font-size: 1.5em;">Application For Employment</span>
        <img src="./images/SonicSales.png" height="40px" alt="">
    </div>

    <MudTabs Elevation="0" Rounded="true" ApplyEffectsToContainer="true" PanelClass="pa-6" @ref="tabs" @bind-ActivePanelIndex="activeIndex" Centered=true>
        <MudTabPanel TabContent="tabHeader(0)">
            <EditForm Model="applicant" OnSubmit="OnValidSubmitPersonal">
                <DataAnnotationsValidator />
                @* <ValidationSummary/> *@
                <MudCard Class="py-3 px-3" Elevation="0">
                    <MudCardContent>
                        <span class="spantitle" style="color:#6A707D">Please fill out the form</span>
                        <div class="row mt-3 mb-2">
                            <div class="col-md-6 ">
                                <MudText Typo="Typo.body2">Position Applied for 1 </MudText>
                                <MudTextField @bind-Value="applicant.App_PosApplied1" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                            </div>
                            <div class="col-md-6 ">
                                <MudText Typo="Typo.body2">Position Applied for 2 </MudText>
                                <MudTextField @bind-Value="applicant.App_PosApplied2" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                            </div>
                        </div>
                        <div class="row mb-2">
                            <div class="col-md-6 ">
                                <MudText Typo="Typo.body2">Present Salary</MudText>
                                <MudTextField @bind-Value="applicant.App_PresSalary" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                            </div>
                            <div class="col-md-6 ">
                                <MudText Typo="Typo.body2">Expected Salary</MudText>
                                <MudTextField @bind-Value="applicant.App_ExpSalary" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                            </div>
                        </div>
                        <div class="col">
                            <MudText Typo="Typo.body2">Source of Application</MudText>
                            <MudTextField @bind-Value="applicant.App_SourceApp" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                        </div>
                    </MudCardContent>
                </MudCard>


                <MudDivider DividerType="DividerType.Middle" Class="my-4" />

                <MudCard Class="py-3 px-3 pb-2" Elevation="0">
                    <MudCardContent>
                        <span class="spantitle">Personal Information</span>
                        <div class="row mb-2 mt-3">
                            <div class="col-sm col-md-3 ">
                                <MudText Typo="Typo.body2">Last Name</MudText>
                                <MudTextField @bind-Value="applicant.App_LastName" For="@(() => applicant.App_LastName)" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                            </div>
                            <div class="col-sm col-md-4 ">
                                <MudText Typo="Typo.body2">First Name</MudText>
                                <MudTextField @bind-Value="applicant.App_FirstName" For="@(() => applicant.App_FirstName)" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                            </div>
                            <div class="col-sm col-md-3 ">
                                <MudText Typo="Typo.body2">Middle Name</MudText>
                                <MudTextField @bind-Value="applicant.App_MiddleName" For="@(() => applicant.App_MiddleName)" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                            </div>
                            <div class="col-sm col-md-2 ">
                                <MudText Typo="Typo.body2">Suffix</MudText>
                                <MudTextField @bind-Value="applicant.App_Suffix" For="@(() => applicant.App_Suffix)" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                            </div>
                        </div>
                        <div class="row mb-2">
                            <div class="col-sm col-md-6 ">
                                <MudText Typo="Typo.body2">Contact Nos</MudText>
                                <MudTextField @bind-Value="applicant.App_ContactNo" For="@(() => applicant.App_ContactNo)" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                            </div>
                            <div class="col-sm col-md-6 ">
                                <MudText Typo="Typo.body2">Email</MudText>
                                <MudTextField @bind-Value="applicant.App_Email" For="@(() => applicant.App_Email)" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                            </div>
                        </div>
                        <div class="row mb-2">
                            <div class="col-sm col-md-4 ">
                                <MudText Typo="Typo.body2">Date of Birth (mm/dd/yyyy)</MudText>
                                <MudDatePicker Class="mdtextfield" DisableUnderLine @bind-Date="@applicant.App_DOB" For="@(() => applicant.App_DOB)" Mask="@(new DateMask("MM/dd/yyyy"))" DateFormat="MM/dd/yyyy" Editable="true" Variant="Variant.Outlined" Margin="Margin.Dense" />
                             </div>
                             <div class="col-sm col-md-4 ">
                                 <MudText Typo="Typo.body2">Age</MudText>
                                 <MudTextField Class="mdtextfield" DisableUnderLine Value="@CalculateAge(applicant.App_DOB)" ReadOnly="true" Variant="Variant.Outlined" Margin="Margin.Dense" />
                             </div>
                             <div class="col-sm col-md-4 ">
                                 <MudText Typo="Typo.body2">Gender</MudText>
                                 <MudSelect @bind-Value="applicant.App_GenderId" For=@(() => applicant.App_GenderId) Immediate=true Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense">
                                     <MudSelectItem Value=0 Disabled>---Select---</MudSelectItem>
                                     @foreach (var gender in GendersL)
                                    {
                                        <MudSelectItem Value="@gender.Id">@gender.Name</MudSelectItem>
                                    }
                                </MudSelect>
                                @* <ValidationMessage For=@(() => applicant.App_GenderId) class="mb-2"/> *@

                            </div>
                        </div>
                        <div class="row mb-2">
                            <div class="col-sm col-md-4 ">
                                <MudText Typo="Typo.body2">Citizenship</MudText>
                                <MudTextField @bind-Value="applicant.App_Citizenship" For=@(() => applicant.App_Citizenship) Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                            </div>
                            <div class="col-sm col-md-4 ">
                                <MudText Typo="Typo.body2">Civil Status</MudText>
                                <MudSelect @bind-Value="applicant.App_CivilStatusId" For=@(() => applicant.App_CivilStatusId) Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense">
                                    <MudSelectItem Value=0 Disabled>---Select---</MudSelectItem>
                                    @foreach (var cs in CivilStatusL)
                                    {
                                        <MudSelectItem Value="@cs.Id">@cs.Name</MudSelectItem>
                                    }
                                </MudSelect>
                            </div>
                            <div class="col-sm col-md-4 ">
                                <MudText Typo="Typo.body2">Religion</MudText>
                                <MudSelect @bind-Value="applicant.App_ReligionId" For=@(() => applicant.App_ReligionId) Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense">
                                    <MudSelectItem Value=0 Disabled>---Select---</MudSelectItem>
                                    @foreach (var rel in ReligionsL)
                                    {
                                        <MudSelectItem Value="@rel.Id">@rel.Name</MudSelectItem>
                                    }
                                </MudSelect>
                            </div>
                        </div>
                    </MudCardContent>
                </MudCard>

                <MudDivider DividerType="DividerType.Middle" Class="my-4" />

                <MudCard Class="py-3 px-3 pb-2" Elevation="0">
                    <MudCardContent>
                        <div class="row mb-2">
                            <div class="col-sm col-md-6 ">
                                <MudText Typo="Typo.body2">TIN</MudText>
                                <MudTextField @bind-Value="applicant.App_TIN" For=@(() => applicant.App_TIN) Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" MaxLength="12" />
                            </div>
                            <div class="col-sm col-md-6 ">
                                <MudText Typo="Typo.body2">Pag-Ibig</MudText>
                                <MudTextField @bind-Value="applicant.App_PagIbig" For=@(() => applicant.App_PagIbig) Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" MaxLength="12" />
                            </div>
                        </div>
                        <div class="row mb-2">
                            <div class="col-sm col-md-6 ">
                                <MudText Typo="Typo.body2">SSS</MudText>
                                <MudTextField @bind-Value="applicant.App_SSS" For=@(() => applicant.App_SSS) Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" MaxLength="12" />
                            </div>
                            <div class="col-sm col-md-6 ">
                                <MudText Typo="Typo.body2">Philhealth</MudText>
                                <MudTextField @bind-Value="applicant.App_Philhealth" For=@(() => applicant.App_Philhealth) Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" MaxLength="12" />
                            </div>
                        </div>
                    </MudCardContent>
                </MudCard>
                <MudItem xs="12" Class="d-flex justify-content-end mt-4">
                    <MudButton EndIcon="@Icons.Material.Rounded.NavigateNext" Class="btnnext" ButtonType="ButtonType.Submit">NEXT</MudButton>
                </MudItem>
            </EditForm>
        </MudTabPanel>
        <MudTabPanel TabContent="tabHeader(1)">
            <EditForm Model="address" OnValidSubmit="ActivateAddressField">
                <DataAnnotationsValidator />
                @* <ValidationSummary/> *@
                <MudCard Class="py-3 px-3 pb-2" Elevation="0">
                    <MudCardContent>
                        <span class="spantitle">Address Information</span> <br />
                        <span class="spantitle" style="font-size: 14px;">Present Address</span>
                        <div class="row mb-2">
                            <div class="col">
                                <MudText Typo="Typo.body2">(Unit/House/Building/Street Number, Street Name, Barangay Name)</MudText>
                                <MudTextField @bind-Value="address.CurrentAdd" For="@(() => address.CurrentAdd)" Class="" Variant="Variant.Outlined" Margin="Margin.Dense" />
                            </div>
                        </div>
                        <div class="row mb-2">
                            <div class="col-md-3 ">
                                <MudText Typo="Typo.body2">City</MudText>
                                <MudTextField @bind-Value="address.CurrentCity" For="@(() => address.CurrentCity)" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                            </div>
                            <div class="col-md-3 ">
                                <MudText Typo="Typo.body2">Province</MudText>
                                <MudTextField @bind-Value="address.CurrentProvince" For="@(() => address.CurrentProvince)" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                            </div>
                            <div class="col-md-3 ">
                                <MudText Typo="Typo.body2">ZipCode</MudText>
                                <MudTextField @bind-Value="address.CurrentZipCode" For="@(() => address.CurrentZipCode)" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                            </div>
                            <div class="col-md-3 ">
                                <MudText Typo="Typo.body2">Country</MudText>
                                <MudTextField @bind-Value="address.CurrentCountry" For="@(() => address.CurrentCountry)" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                            </div>
                        </div>
                        <span class="spantitle" style="font-size: 14px;">Permanent Address</span>
                        <div class="row mb-2">
                            <div class="col">
                                <MudText Typo="Typo.body2">(Unit/House/Building/Street Number, Street Name, Barangay Name)</MudText>
                                <MudTextField @bind-Value="address.PermanentAdd" For="@(() => address.PermanentAdd)" Class="" Variant="Variant.Outlined" Margin="Margin.Dense" />
                            </div>
                        </div>
                        <div class="row mb-2">
                            <div class="col-md-3 ">
                                <MudText Typo="Typo.body2">City</MudText>
                                <MudTextField @bind-Value="address.PermanentCity" For="@(() => address.PermanentCity)" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                            </div>
                            <div class="col-md-3 ">
                                <MudText Typo="Typo.body2">Province</MudText>
                                <MudTextField @bind-Value="address.PermanentProvince" For="@(() => address.PermanentProvince)" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                            </div>
                            <div class="col-md-3 ">
                                <MudText Typo="Typo.body2">ZipCode</MudText>
                                <MudTextField @bind-Value="address.PermanentZipCode" For="@(() => address.PermanentZipCode)" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                            </div>
                            <div class="col-md-3 ">
                                <MudText Typo="Typo.body2">Country</MudText>
                                <MudTextField @bind-Value="address.PermanentCountry" For="@(() => address.PermanentCountry)" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                            </div>
                        </div>
                    </MudCardContent>
                </MudCard>
                <MudItem xs="12" Class="d-flex justify-content-center mt-4 mb-4">
                    <MudButton EndIcon="@Icons.Material.Rounded.NavigateNext" Class="btnnext" ButtonType="ButtonType.Submit">NEXT</MudButton>
                </MudItem>
            </EditForm>
            <MudCard Class="py-3 px-3 pb-2" Elevation="0">
                <MudCardContent>
                    <span class="spantitle">Family Information</span> <br />
                    <span class="spantitle" style="font-size: 14px;">Spouse</span>
                    <div class="row mb-2">
                        <div class="col-sm col-md-6 ">
                            <MudText Typo="Typo.body2">Spouse Name</MudText>
                            <MudTextField @bind-Value="applicant.App_SpouseName" Class="" Variant="Variant.Outlined" Margin="Margin.Dense" />
                        </div>
                        <div class="col-sm col-md-6 ">
                            <MudText Typo="Typo.body2">Spouse Occupation</MudText>
                            <MudTextField @bind-Value="applicant.App_SpouseOccupation" Class="" Variant="Variant.Outlined" Margin="Margin.Dense" />
                        </div>
                    </div>
                    <span class="spantitle" style="font-size: 14px;">Father</span>
                    <div class="row mb-2">
                        <div class="col-sm col-md-6 ">
                            <MudText Typo="Typo.body2">Father Name</MudText>
                            <MudTextField @bind-Value="applicant.App_FatherName" Class="@clsFatherName" Immediate=true Variant="Variant.Outlined" Margin="Margin.Dense" />
                            <span class="mud-input-helper-text mud-input-error">@mesFatherName</span>
                        </div>
                        <div class="col-sm col-md-6 ">
                            <MudText Typo="Typo.body2">Father Occupation</MudText>
                            <MudTextField @bind-Value="applicant.App_FatherOccupation" Class="" Variant="Variant.Outlined" Margin="Margin.Dense" />
                        </div>
                    </div>
                    <span class="spantitle" style="font-size: 14px;">Mother</span>
                    <div class="row mb-2">
                        <div class="col-sm col-md-6 ">
                            <MudText Typo="Typo.body2">Mother Name</MudText>
                            <MudTextField @bind-Value="applicant.App_MotherName" Class="@clsMotherName" Immediate=true Variant="Variant.Outlined" Margin="Margin.Dense" />
                            <span class="mud-input-helper-text mud-input-error">@mesMotherName</span>
                        </div>
                        <div class="col-sm col-md-6 ">
                            <MudText Typo="Typo.body2">Mother Occupation</MudText>
                            <MudTextField @bind-Value="applicant.App_MotherOccupation" Class="" Variant="Variant.Outlined" Margin="Margin.Dense" />
                        </div>
                    </div>
                    <div class="d-flex flex-grow-1 justify-content-between align-items-center">
                        <span class="spantitle" style="font-size: 14px;">Sibling</span>
                        <div>
                            <MudTooltip Text="Click to remove Sibling">
                                <MudIconButton Class="p-0 addminbtn" OnClick="RemoveSibling" Icon="@Icons.Material.Rounded.RemoveCircle" />
                            </MudTooltip>
                            <MudTooltip Text="Click to add more Sibling">
                                <MudIconButton Class="p-0 addminbtn" OnClick="() => AddNewSibling(applicant.App_VerifyId)" Icon="@Icons.Material.Rounded.AddCircle" />
                            </MudTooltip>
                        </div>
                    </div>
                    @if (listofSiblings.Any())
                    {
                        @foreach (var lis in listofSiblings)
                        {
                            <div class="row mb-2">
                                <div class="col-sm col-md-6 ">
                                    <MudText Typo="Typo.body2">Sibling Name</MudText>
                                    <MudTextField @bind-Value="lis.App_SiblingName" Class="" Immediate=true Variant="Variant.Outlined" Margin="Margin.Dense" />
                                    <span class="mud-input-helper-text mud-input-error"></span>
                                </div>
                                <div class="col-sm col-md-6 ">
                                    <MudText Typo="Typo.body2">Sibling Occupation</MudText>
                                    <MudTextField @bind-Value="lis.App_SiblingOccupation" Class="" Variant="Variant.Outlined" Margin="Margin.Dense" />
                                </div>
                            </div>
                            <MudDivider DividerType="DividerType.Middle" Class="my-4" />
                        }
                    }
                    <div class="d-flex flex-grow-1 justify-content-between align-items-center">
                        <span class="spantitle" style="font-size: 14px;">Children</span>
                        <div>
                            <MudTooltip Text="Click to remove Children">
                                <MudIconButton Class="p-0 addminbtn" OnClick="RemoveChild" Icon="@Icons.Material.Rounded.RemoveCircle" />
                            </MudTooltip>
                            <MudTooltip Text="Click to add more Children">
                                <MudIconButton Class="p-0 addminbtn" OnClick="() => AddNewChild(applicant.App_VerifyId)" Icon="@Icons.Material.Rounded.AddCircle" />
                            </MudTooltip>
                        </div>
                    </div>
                    @if (listofChildren.Any())
                    {
                        @foreach (var lis in listofChildren)
                        {
                            <div class="row mb-2">
                                <div class="col-sm col-md-6 ">
                                    <MudText Typo="Typo.body2">Childen Name</MudText>
                                    <MudTextField @bind-Value="lis.App_ChildrenName" Class="" Immediate=true Variant="Variant.Outlined" Margin="Margin.Dense" />
                                    <span class="mud-input-helper-text mud-input-error"></span>
                                </div>
                                <div class="col-sm col-md-6 ">
                                    <MudText Typo="Typo.body2">Childen Occupation</MudText>
                                    <MudTextField @bind-Value="lis.App_ChildrenOccupation" Class="" Variant="Variant.Outlined" Margin="Margin.Dense" />
                                </div>
                            </div>
                            <MudDivider DividerType="DividerType.Middle" Class="my-4" />
                        }
                    }
                </MudCardContent>
            </MudCard>
            <MudItem xs="12" Class="d-flex justify-content-between mt-5">
                <MudButton StartIcon="@Icons.Material.Rounded.NavigateBefore" Class="btnnext" OnClick="@(()=> Activate(0))" style="background-color: #C5CFD2;">BACK</MudButton>
                <MudButton id="bb" EndIcon="@Icons.Material.Rounded.NavigateNext" Class="btnnext" Disabled=validFam OnClick="OnValidSubmitFamily">NEXT</MudButton>
            </MudItem>
        </MudTabPanel>
        <MudTabPanel TabContent="tabHeader(2)">
            <MudCard Class="py-3 px-3 pb-2" Elevation="0">
                <MudCardContent>
                    <div class="d-flex flex-grow-1 justify-content-between align-items-center mb-3">
                        <span class="spantitle">Professional Background (from most recent experience)</span>
                        <div>
                            <MudTooltip Text="Click to remove Experience">
                                <MudIconButton Class="p-0 addminbtn" OnClick="RemoveBgProf" Icon="@Icons.Material.Rounded.RemoveCircle" />
                            </MudTooltip>
                            <MudTooltip Text="Click to add more Experience">
                                <MudIconButton Class="p-0 addminbtn" OnClick="() => AddNewBgProf(applicant.App_VerifyId)" Icon="@Icons.Material.Rounded.AddCircle" />
                            </MudTooltip>
                        </div>
                    </div>
                    @if (listofBgProf.Any())
                    {
                        @foreach (var lis in listofBgProf)
                        {
                            <div class="row mb-2">
                                <div class="col-md-6">
                                    <MudText Typo="Typo.body2">From (mm/dd/yyyy)</MudText>
                                    <MudDatePicker Class="mdtextfield" DisableUnderLine @bind-Date="@lis.App_DateFrom" Mask="@(new DateMask("MM/dd/yyyy"))" DateFormat="MM/dd/yyyy" Editable="true" Variant="Variant.Outlined" Margin="Margin.Dense" />
                                 </div>
                                 <div class="col-md-6">
                                     <MudText Typo="Typo.body2">To (mm/dd/yyyy)</MudText>
                                     <MudDatePicker Class="mdtextfield" DisableUnderLine @bind-Date="@lis.App_DateTo" Mask="@(new DateMask("MM/dd/yyyy"))" DateFormat="MM/dd/yyyy" Editable="true" Variant="Variant.Outlined" Margin="Margin.Dense" />
                                 </div>
                             </div>
                            <div class="row mb-2">
                                <div class="col-md-3 ">
                                    <MudText Typo="Typo.body2">Company</MudText>
                                    <MudTextField @bind-Value="@lis.App_CompanyName" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                                </div>
                                <div class="col-md-3 ">
                                    <MudText Typo="Typo.body2">Position</MudText>
                                    <MudTextField @bind-Value="@lis.App_Position" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                                </div>
                                <div class="col-md-3 ">
                                    <MudText Typo="Typo.body2">Basic Salary</MudText>
                                    <MudTextField @bind-Value="@lis.App_BasicSalary" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                                </div>
                                <div class="col-md-3 ">
                                    <MudText Typo="Typo.body2">Reason for Leaving</MudText>
                                    <MudTextField @bind-Value="@lis.App_RSLeaving" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                                </div>
                            </div>
                            <MudDivider DividerType="DividerType.Middle" Class="my-4" />
                        }
                    }

                </MudCardContent>
            </MudCard>
            <MudDivider DividerType="DividerType.Middle" Class="my-4" />

            <MudCard Class="py-3 px-3 pb-2" Elevation="0">
                <MudCardContent>
                    <span class="spantitle">Educational Background</span>

                    <div class="d-flex flex-grow-1 justify-content-between align-items-center mt-3">
                        <span class="spantitle">Primary</span>
                        <div>
                            <MudTooltip Text="Click to remove Primary">
                                <MudIconButton Class="p-0 addminbtn" OnClick="RemovePrimary" Icon="@Icons.Material.Rounded.RemoveCircle" />
                            </MudTooltip>
                            <MudTooltip Text="Click to add more Primary">
                                <MudIconButton Class="p-0 addminbtn" OnClick="() => AddNewPrimary(applicant.App_VerifyId)" Icon="@Icons.Material.Rounded.AddCircle" />
                            </MudTooltip>
                        </div>
                    </div>
                    @if (listOfPrimary.Any())
                    {
                        @foreach (var lis in listOfPrimary)
                        {
                            <div class="row mb-2">
                                <div class="col-md-6">
                                    <MudText Typo="Typo.body2">Name of School</MudText>
                                    <MudTextField @bind-Value="@lis.PriSchoolName" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                                </div>
                                <div class="col-md-6">
                                    <MudText Typo="Typo.body2">School Location</MudText>
                                    <MudTextField @bind-Value="@lis.PriSchoolLoc" Class="mdtextfield" Variant="Variant.Outlined" Margin="Margin.Dense" />
                                </div>
                            </div>
                            <div class="row mb-2">
                                <div class="col-md-6">
                                    <MudText Typo="Typo.body2">Awards</MudText>
