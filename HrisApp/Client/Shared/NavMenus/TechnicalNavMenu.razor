        <MudNavGroup Class="navlinktitle" Title="Consumable Assets" Expanded="false" Icon="@Icons.Material.Rounded.ShoppingBag" IconColor="Color.Inherit">
            <MudNavLink Class="navlinktitle" Href="/asset-consumable" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Rounded.ShoppingBag" IconColor="Color.Inherit">Consumables</MudNavLink>
        </MudNavGroup>
        @* <MudNavGroup Class="navlinktitle" Title="Vehicale Monitoring" Expanded="false" Icon="@Icons.Material.Rounded.DirectionsCar" IconColor="Color.Inherit">
            <MudNavLink Class="navlinktitle" Href="/asset-vehicle" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.DirectionsCar" IconColor="Color.Inherit">Vehicles</MudNavLink>
        </MudNavGroup> *@
